# Development Environment Variables
NODE_ENV=development
PORT=3001
APP_NAME=NestJS App (Dev)

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=nestjs_dev
DB_USER=dev_user
DB_PASSWORD=dev_password

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# Logging
LOG_LEVEL=debug
LOG_FILE=logs/app-dev.log

# API
API_PREFIX=/api/v1
API_VERSION=1.0.0

# Security
JWT_SECRET=dev-secret-key
JWT_EXPIRES_IN=1h

# External Services
EXTERNAL_API_URL=https://api-dev.example.com
EXTERNAL_API_KEY=dev-api-key

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090

# Docker
DOCKER_IMAGE=nestjs-app
DOCKER_TAG=dev

# Kubernetes
K8S_NAMESPACE=nestjs-app-dev
K8S_REPLICAS=1
